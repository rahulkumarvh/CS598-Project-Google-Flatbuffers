namespace Dataframe;

enum DataType: byte {
    Int64 = 0,
    Float = 1,
    String = 2
}

table ColumnMetadata {
  name: string;  // Name of the column
  dtype: DataType;  // Data type of the column
}

table Dataframe {
  metadata: [ColumnMetadata]; // Metadata for all columns
  columns: [Column]; // Columns data
}

table Column {
  dtype: DataType;
  int64_data: [int64];
  float_data: [float];
  string_data: [string];
}

root_type Dataframe;