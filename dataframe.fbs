namespace CS598; 

enum DataType: byte { Int, Float, String } 

table Metadata { 
    name: string; 
    dtype: DataType; 
} 

table Column { 
    metadata: Metadata; 
    int_values: [int64]; 
    float_values: [float64]; 
    string_values: [string]; 
} 

table DataFrame { 
    metadata: string; 
    columns: [Column]; 
} 

root_type DataFrame;
