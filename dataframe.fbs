namespace CS598;

table IntColumn {
  name:string;
  values:[int];
}

table FloatColumn {
  name:string;
  values:[float];
}

table StringColumn {
  name:string;
  values:[string];
}

// Define a table for a DataFrame
table DataFrame {
  int_columns:[IntColumn];
  float_columns:[FloatColumn];
  string_columns:[StringColumn];
}

root_type DataFrame;