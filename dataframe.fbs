namespace DataFrame;

enum ValueType: byte { 
    Int = 0, 
    Float = 1, 
    String = 2 
}

table Metadata {
    name: string;
    dtype: ValueType;
}

table Column {
    metadata: Metadata;
    int_values: [int64]; // Values for int columns
    float_values: [float]; // Values for float columns
    string_values: [string]; // Values for string columns
}

table DataFrame {
    metadata: string; // Metadata for the dataframe
    columns: [Column]; // List of columns
}

root_type DataFrame;
