namespace CS598;

enum DataType: byte {
    Int64 = 0,
    Float = 1,
    String = 2
}

table ColumnMetadata {
    name: string;
    dtype: DataType;
}

table Column {
    dtype: DataType;
    int64_data: [int64];
    float_data: [float];
    string_data: [string];
}

table Dataframe {
    metadata: [ColumnMetadata];
    columns: [Column];
}

root_type Dataframe;
