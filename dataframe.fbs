namespace MP3;

enum DataType: byte {
    Int64 = 0,
    Float = 1,
    String = 2
}

table ColumnMetadata {
    name: string;
    dtype: DataType;
}

table Column {
    dtype: DataType;
    int64_data: [int64] (deprecated);
    float_data: [float] (deprecated);
    string_data: [string] (deprecated);
}

table Dataframe {
    metadata: [ColumnMetadata];
    columns: [Column];
}

root_type Dataframe;
